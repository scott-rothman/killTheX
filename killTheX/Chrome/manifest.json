{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "KillTheX",
    "version": "1.0.0",
    "permissions": [
        "tabs",
        "scripting",
        "activeTab",
        "webNavigation",
        "webRequest"
    ],
    "host_permissions": [
        "https://twitter.com/*",
        "https://www.twitter.com/*",
        "https://x.com/*",
        "https://www.x.com/*"
    ],
    "background": {
        "service_worker": "service_worker.js",
        "type": "module"
    }
}
